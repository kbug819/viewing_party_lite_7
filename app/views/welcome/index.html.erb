<h1>Viewing Party</h1>

<% if current_user %>
  <%= button_to "Log Out", "/", method: :delete %>
  <% else %>
  <%= button_to "Create a New User", "/register", method: :get %>
  <%= button_to "Log In", "/login", method: :get %>
  <% end %>

<% if current_user %>
<p> Other User Emails: </p>
  <% @users.each do |user| %>
    <ul>
    <li><%= "#{user.email}"%></li>
    </ul>
  <% end %>
<% end %>